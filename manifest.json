{
  "manifest_version": 2,
  "name": "Dark Mode",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["handler.js"]
  },
  "browser_action": {
    "default_icon": "moon.png",
    "default_popup": "popup.html",
    "default_title": "Dark mode"
  }
}
